{
  "name": "lolog",
  "version": "1.0.0",
  "description": "Lookback Log",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "author": "Martin Algesten",
  "private": true,
  "scripts": {
    "test": "npm run lint && ava --verbose",
    "test-watch": "ava --watch",
    "build": "tsc && cp ./src/*.js ./build/ && browserify -r ./build/index.js:lolog ./build/index.js> ./lolog.js",
    "watch": "tsc -w",
    "lint": "tslint --project ."
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^10.12.18",
    "ava": "^1.0.1",
    "browserify": "^16.2.3",
    "ts-node": "^7.0.1",
    "tslint": "^5",
    "tslint-immutable": "^4.7",
    "typescript": "^3.1.6"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
